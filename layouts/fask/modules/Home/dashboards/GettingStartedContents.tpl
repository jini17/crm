<script>
    jQuery(document).ready(function(){
        jQuery(".need-more-help-btn").on('click',function(){
            jQuery('.help-form-container').addClass("animated fadeInRight").removeClass('hide');
            jQuery(this).addClass('hide');
         });
         jQuery('.close-help-btn').on('click',function(){
             jQuery(".need-more-help-btn").removeClass('hide');
             jQuery('.help-form-container').removeClass("animated fadeInRight").addClass('animated fadeInLeft hide');
         });

         jQuery('.create-dashboard').on('click',function(){
            jQuery('.addNewDashBoard').trigger('click')
        })
           jQuery('.rename-dashboard').on('click',function(){
            
            var TabName = jQuery('.active.dashboardTab').find(".name.textOverflowEllipsis").addClass('hide').find("strong").text();
            jQuery('.active.dashboardTab').find(".editTabName").removeClass('hide').find('input').val(TabName)
        })
        jQuery('.delete-dashboard').on('click',function(){
             jQuery('.active.dashboardTab').find(".deleteTab").addClass('blink text-danger').css('border',"1px solid red").focus()
        });
    })
</script>    